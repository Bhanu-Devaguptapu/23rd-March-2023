#include<stdio.h>
#include<conio.h>
#include<string.h>
int main(void){
	struct Book{
		int id;
		char title[20];
		char author[20];
		float price;
	};
	struct Book b;
    strcpy(b.author,"Nannaya");
	strcpy(b.title,"Mahabaratham");
	b.price=5000;
	printf("Book Details:");
	printf("TITLE = %s\n AUTHOR = %s\n  PRICE=%f",b.title,b.author,b.price);
	getch();
	}
